#include<stdio.h>

void linearsearch(int a[100],int n,int data)
{
    
    int i=0,flag=0;
    
    for(i=0;i<n;i++)
    {
        if(a[i]==data)
        {
            flag=1;
            break;
        }
    }
    if(flag==0)
         printf("not found");
    else
        printf("found at : %d",i+1);
}

void main()
{
    int i,n,a[100],data;
    
    printf("enter size : ");
    scanf("%d",&n);
    
    printf("enter an array:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    printf("enter data :\n");
    scanf("%d",&data);
    
    linearsearch(a,n,data);
    
}
